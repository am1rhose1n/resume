script.
  function get(baseUrl) {
  // console.log("URL : ", baseUrl)
  var data;
  $.ajax({
  url: baseUrl,
  async: false,
  dataType: 'text',
  cache: false,
  success: function (data_) {
  data = data_;
  }
  });
  return data;
  }
  function post(path, data) {
  var postData = data
  var outData;
  $.ajax({
  url: path,
  data: postData,
  method: 'POST',
  async: false,
  success: function (data) {
  outData = data;
  }
  });
  return outData;
  }
  function submitForm(){
  var details="name : "+$('#name.form-control').val()+", mail : "+$('#email.form-control').val()+", text : "+$('#message.form-control').val()+""
  post("/mass",details)
  }